{
  "name": "mobitask-app",
  "version": "1.0.0",
  "description": "Comprehensive mobile task management application with modular architecture",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "web": "npx expo start --web",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "build:web": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npx expo export --platform web --output-dir dist",
    "serve:prod": "node server.js",
    "deploy": "npm run build:web && npm run serve:prod",
    "replit:deploy": "npx expo export --platform web --output-dir dist && node server.js",
    "test": "jest",
    "lint": "eslint .",
    "clear": "npx expo start --clear"
  },
  "keywords": [
    "react-native",
    "expo",
    "mobile",
    "task-management",
    "firebase"
  ],
  "author": "Ramos_bot <peralta.ramos.bot@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@types/node": "^22.13.11",
    "expo": "~53.0.0",
    "expo-constants": "~17.1.7",
    "expo-device": "^7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-print": "^14.1.4",
    "expo-router": "~5.1.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "express": "^4.21.2",
    "firebase": "^10.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-runtime": "~5.0.4",
    "@types/react": "~19.0.10",
    "cross-env": "^7.0.3",
    "jest": "^29.4.0",
    "serve": "^14.2.4",
    "typescript": "~5.8.3"
  },
  "private": true
}