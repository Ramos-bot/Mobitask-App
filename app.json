{
  "expo": {
    "name": "Mobitask App",
    "slug": "mobitask-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/aqua.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/aqua.png",
        "backgroundColor": "#4CAF50"
      }
    },
    "web": {
      "favicon": "./assets/aqua.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications"
    ],
    "scheme": "mobitask",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}